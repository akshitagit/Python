# function to check given number is prime or not
def isPrime(number):
    if number > 1:

        for i in range(2, number):

            if (number % i) == 0:
                print(number, "is not a prime number")
                break
        else:
            print(number, "is a prime number")

    else:
        print(number, "is not a prime number")

# it takes input from user
number = int(input())

# function call
isPrime(number)
